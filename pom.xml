<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0"         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">    <modelVersion>4.0.0</modelVersion>    <groupId>laba5</groupId>    <artifactId>laba-5</artifactId>    <version>1.1</version>		<build>		<plugins>			<plugin>				<groupId>org.apache.maven.plugins</groupId>				<artifactId>maven-jar-plugin</artifactId>				<version>3.2.0</version>				<configuration>					<archive>						<manifest>							<mainClass>laba5.Main</mainClass>						</manifest>						<manifestEntries>							<Built-By>${user.name}</Built-By> <!-- Имя пользователя, собравшего проект -->							<Build-Jdk>${java.version}</Build-Jdk> <!-- Версия JDK, используемая для сборки -->							<Created-By>Maven</Created-By> <!-- Информация о сборщике -->							<Implementation-Version>${project.version}</Implementation-Version> <!-- Версия проекта -->							<Specification-Version>1.0</Specification-Version> <!-- Версия спецификации -->						</manifestEntries>					</archive>				</configuration>			</plugin>			<plugin>				<groupId>org.apache.maven.plugins</groupId>				<artifactId>maven-compiler-plugin</artifactId>				<version>3.8.1</version> <!-- Укажите нужную версию -->				<configuration>					<source>17</source> <!-- Версия Java исходников -->					<target>17</target> <!-- Версия Java целевого кода -->					<encoding>UTF-8</encoding> <!-- Указываем кодировку -->				</configuration>			</plugin>		</plugins>	</build></project>